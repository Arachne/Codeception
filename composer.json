{
  "name": "contributte/codeception",
  "description": "Integration of Nette framework to Codeception.",
  "keywords": [
    "contributte",
    "codeception",
    "nette",
    "testing",
    "integration"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/codeception",
  "authors": [
    {
      "name": "Jáchym Toušek",
      "homepage": "http://enumag.cz",
      "email": "enumag@gmail.com"
    }
  ],
  "require": {
    "php": ">= 8.0",
    "codeception/codeception": "^5.0.0",
    "codeception/lib-innerbrowser": "^3.1.3 || ^4.0.0",
    "nette/di": "^3.1.1",
    "nette/bootstrap": "^3.2.0",
    "nette/http": "^3.2.1",
    "nette/utils": "^3.2.8 || ^4.0"
  },
  "require-dev": {
    "latte/latte": "^2.11.5",
    "nette/application": "^3.1.7",
    "nette/caching": "^3.1.4",
    "ninjify/qa": "^0.14",
    "phpstan/phpstan": "^1.8.11",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.1.0",
    "phpstan/phpstan-strict-rules": "^1.4.4",
    "tracy/tracy": "^2.9.0",
    "phpunit/php-code-coverage": "^9.2.24"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Codeception\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Functional\\": "tests/Functional/src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.5.x-dev"
    }
  }
}
