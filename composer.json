{
  "name": "contributte/codeception",
  "description": "Integration of Nette framework to Codeception.",
  "keywords": [
    "contributte",
    "codeception",
    "nette",
    "testing",
    "integration"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/codeception",
  "authors": [
    {
      "name": "Jáchym Toušek",
      "homepage": "http://enumag.cz",
      "email": "enumag@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "codeception/codeception": "^2.5.1 || ^3.0.2",
    "nette/di": "^3.0",
    "nette/bootstrap": "^3.0",
    "nette/http": "^3.0.3",
    "nette/utils": "^3.0"
  },
  "require-dev": {
    "latte/latte": "~2.5.1",
    "nette/application": "^3.0",
    "nette/caching": "~2.5.8 || ^3.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "tracy/tracy": "~2.6.1"
  },
  "conflict": {
    "symfony/browser-kit": "<4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Codeception\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Functional\\": "tests/functional/src"
    },
    "files": [
      "vendor/codeception/codeception/shim.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
